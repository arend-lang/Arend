\open Data.Bool

\class Semigroup {
  \function X : \Type0
  \function op : X -> X -> X
  \function assoc (x y z : X) : op (op x y) z = op x (op y z)
}

\class Monoid {
  \function S : Semigroup
  \function id : S.X
  \function lunit (x : S.X) : S.op id x = x
  \function runit (x : S.X) : S.op x id = x
}

\function
test => \new Semigroup --Bool and
