\import Algebra.Meta
\import Algebra.Monoid
\import Algebra.Pointed
\import Meta
\import Paths

\lemma test1 {M : Monoid} (x y : M) (p : Nat -> x * y = ide)
  : (x * x) * (y * y) = ide
  => using (p 0) solve

\lemma test2 {M : Monoid} (x y : M) (p : x * y = ide)
  => fail {(x * x) * (y * y) = ide} hiding p solve
