\open ::Data::List \hiding(concat, snoc)
\open ::Data::Nat::Base
\open ::Data::Nat::Compare
\open ::Data::Nat::Properties
\open ::Logic


\open ::LC::Term::Term


\data (#) (x : Nat) (t : NTerm)
    | (#) x (NVar v)        => FreshVar (x != v)
    | (#) x (NApp t1 t2)    => FreshApp (x # t1) (x # t2)
    | (#) x (NLam v t)      => FreshLamE (x = v)
    | (#) x (NLam v t)      => FreshLam (x # t)
