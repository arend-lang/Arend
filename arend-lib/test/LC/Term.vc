\open ::Data::Fin
\open ::Data::Unit
\open ::Data::Nat::Base
\open ::LC::Either

-- Наконец-то сами определения термов
\data Term
    | Var Nat
    | App Term Term
    | Lam Nat Term

-- Возможно что придется определять fmap и bind
-- Потом повозимся с этим вариантом
\data Term' (V : \Set0)
    | Var' V
    | App' (Term' V) (Term' V)
    | Lam' (Term' (Either V Unit))

\data Term'' (n : Nat)
    | Var'' (Fin n)
    | App'' (Term'' n) (Term'' n)
    | Lam'' (Term'' (suc n))
