\open ::Paths
\open ::Hlevel
\open ::HITs::Spheres
\open ::Combinators

\data Squash (A : \Type0) 
    | in A
    | eq (a a' : Squash A) (i : I) => \elim i {
        | left => a
        | right => a'
    }

\data Trunc (n : Nat) (A : \Type) 
    | inT A
    | hT (r : (S n).1 -> Trunc n A)
    | spokeT (r : (S n).1 -> Trunc n A) (x : (S n).1) (i : I) => \elim i {
        | left => r x
        | right => hT r
    }

\function
Squash-isProp {A : \Type0} : isProp (Squash A) => \lam a a' => path (\lam i => eq a a' i)

\function
TruncofHlevel (n : Nat) (A : \Type) : ofHlevel (suc n) (Trunc n A) => 
    \let TA => Trunc n A
    \in transport id (inv (ofHlevel-Omega-contr TA n)) (\lam (ta : TA) => {?})

