\function
(+) (x y : Nat) : Nat
    <= \elim x | zero => y | suc x' => suc (x' + y)

\function
(*) (x y : Nat) : Nat
    <= \elim x | zero => zero | suc x' => y + (x' * y)

\function
fac (x : Nat) : Nat
    <= \elim x | zero => suc zero | suc x' => suc x' * fac x'

\function
main : Nat
    => fac (suc (suc (suc (suc zero))))
