\open Data.Unit

\static \data
Either (A B : \Type0) : \Type0
| inl A
| inr B

\static \function
inl' : Either Unit Unit <= inl unit

\static \function
inr' : Either Unit Unit <= inr unit

\static \function
if' {A L R : \Type0} (e : Either L R) (then else : A) : A <= \elim e
    | inr _ => then
    | inl _ => else
