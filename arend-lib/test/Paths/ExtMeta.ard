\import Paths.Meta

\func propTest (x y : Nat) (p q : x = y) : p = q => ext

\func depPropTest (x y : Nat) (p : x = y) : Path (\lam i => p @ i = y) p idp => ext

\func funExtTest {A : \Type} (B : A -> \Type) (f g : \Pi (x : A) -> (B x)) (h : \Pi (x : A) -> (f x = g x)) : f = g
  => ext (\lam x => h x)


