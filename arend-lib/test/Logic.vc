\open ::Paths
\open ::Data::Unit
\open ::Data::Empty
\open ::Data::Either

\function
(&&) {l : Level} (A B : \Type l) => \Sigma A B

\function
(||) {l : Level} (A B : \Type l) => Either A B

\function
(!=) {l : Level} {A : \Type l} (a b : A) <= (~) (a = b)

\function
(<->) {l : Level} (A B : \Type l) => (A -> B) && (B -> A)

\function
contraposition {l : Level} {A B : \Type l} (f : A -> B) (nb : (~) B) : (~) A =>
    \lam a => nb (f a)

\function
contraposition2 {l : Level} {A B : \Type l} (f : A -> (~) B) (b : B) : (~) A =>
    \lam a => f a b

\function
Dec {l : Level} (A : \Type l) <= A || (~) A

\function
double-neg {l : Level} {A : \Type l} (a : A) : (~) ((~) A) =>
    \lam f => f a

\function
cancel-triple-neg {l : Level} {A : \Type l} (nnna : (~) ((~) ((~) A))) : (~) A =>
    \lam a => nnna (double-neg a)

\function
DNLaw {l : Level} (A : \Type l) <= (~) ((~) A) -> A

\function
dec-impl-DNLaw {l : Level} (A : \Type l) (p : Dec A) : (DNLaw A) => \case p | inl a => \lam _ => a | inr f => \lam g => absurd (g f)
