\open Paths
\open Data.Unit
\open Data.Empty
\open Data.Either

\function
(&&) (A B : \Type0) => \Sigma A B

\function
(||) (A B : \Type0) => Either A B

\function
(!=) {A : \Type0} (a b : A) <= (~~) (a = b)

\function
(<->) (A B : \Type0) => (A -> B) && (B -> A)

\function
contraposition {A B : \Type0} (f : A -> B) (nb : (~~) B) : (~~) A =>
    \lam a => nb (f a)

\function
contraposition2 {A B : \Type0} (f : A -> (~~) B) (b : B) : (~~) A =>
    \lam a => f a b

\function
double-neg {A : \Type0} (a : A) : (~~) ((~~) A) =>
    \lam f => f a

\function
cancel-triple-neg {A : \Type0} (nnna : (~~) ((~~) ((~~) A))) : (~~) A =>
    \lam a => nnna (double-neg a)
