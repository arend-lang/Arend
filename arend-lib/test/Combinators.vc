\function
id {X : \Type} (x : X) => x

\function
const {X Y : \Type} (x : X) (y : Y) => x

\function \infixr 1
$ {X Y : \Type} (f : X -> Y) (x : X) => f x

\function \infixr 8
o {X Y Z : \Type} (g : Y -> Z) (f : X -> Y) (x : X) => g $ f x

\function
flip {X Y Z : \Type} (f : X -> Y -> Z) (y : Y) (x : X) => f x y

\function
curry {A B C : \Type} (f : \Sigma A B -> C) : A -> B -> C => \lam a b => f (a, b)

\function
uncurry {A B C : \Type} (f : A -> B -> C) : \Sigma A B -> C => \lam p => f p.1 p.2
