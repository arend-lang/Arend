\function
id {lp : Lvl} {lh : CNat} {X : \Type (lp,lh)} (x : X) => x

\function
const {lp : Lvl} {lh : CNat} {X Y : \Type (lp,lh)} (x : X) (y : Y) => y

\function \infixr 1
($) {lp : Lvl} {lh : CNat} {X Y : \Type (lp,lh)} (f : X -> Y) (x : X) => f x

\function \infixr 8
o {lp : Lvl} {lh : CNat} {X Y Z : \Type (lp,lh)} (g : Y -> Z) (f : X -> Y) (x : X) => g $ f x

\function
flip {lp : Lvl} {lh : CNat} {X Y Z: \Type (lp,lh)} (f : X -> Y -> Z) (y : Y) (x : X) => f x y

