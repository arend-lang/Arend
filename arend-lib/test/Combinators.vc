\function
id {l : Level} {X : \Type l} (x : X) => x

\function
const {l : Level} {X Y : \Type l} (x : X) (y : Y) => y

\function \infixr 1
($) {l : Level} {X Y : \Type l} (f : X -> Y) (x : X) => f x

\function \infixr 8
o {l : Level} {X Y Z : \Type l} (g : Y -> Z) (f : X -> Y) (x : X) => g $ f x

\function
flip {l : Level} {X Y Z: \Type l} (f : X -> Y -> Z) (y : Y) (x : X) => f x y

