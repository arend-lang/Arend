\import Algebra.Monoid
\import Algebra.Pointed
\import Algebra.Ring
\import Category

\record RingHom (R S : Ring)
  | func : R -> S
  | func-ide : func ide = ide
  | func-+ (x y : R) : func (x + y) = func x + func y
  | func-* (x y : R) : func (x * y) = func x * func y

