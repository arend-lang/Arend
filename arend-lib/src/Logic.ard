\import HLevel

\class Unit

\func unit => \new Unit

\data Empty

\func absurd {A : \Type} (x : Empty) : A

\func Not (A : \Type) => A -> Empty

\func removeTrP {A : \Type} (p : isProp A) (t : TrP A) : A \elim t
  | inP a => a
  | truncP t t' i => p (removeTrP p t) (removeTrP p t') @ i

\truncated \data \infixr 2 || (A B : \Type) : \Prop
  | byLeft A
  | byRight B
